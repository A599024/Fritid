<AnchorPane prefHeight="344.0" prefWidth="612.0" style="-fx-background-color: #6577B3#6577B3;" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controllers.SignupController">
